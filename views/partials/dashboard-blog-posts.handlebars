<tr>
    <td id="bp-id">{{id}}</td>
    <td id="bp-title">{{title}}</td>
    <td id="bp-author">{{author}}</td>
    <td id="bp-description">{{description}}</td>
    <td id="bp-createdAt">{{createdAt}}</td>
    <td class="row-controls">
        <span class="table-remove">
            <button type="button" class="  btn btn-edit-row edit-blog-post-modal btn-success line-coverage btn-rounded btn-md my-0" data-toggle="modal" data-target="#edit-blog-post-modal"><i
                    class="btn-edit-row L0 fas fa-edit"></i></button>
            <button type="button" class="  btn btn-delete-row  btn-danger btn-rounded btn-md my-0"><i
                    class="btn-delete-row L2 fas fa-times"></i></button>
        </span>
    </td>
</tr>
